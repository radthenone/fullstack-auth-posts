{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --config vite.config.js",
        "docker": "vite --config vite.docker.config.js",
        "build": "tsc && vite build --mode development --config vite.config.ts && vite build --mode docker --config vite.docker.config.js",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src --check",
        "prettier:fix": "npm run prettier -- --write",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "test-database": "json-server --watch src/data/temp.json --host 0.0.0.0 --port 3050 --routes src/data/routes.json"
    },
    "dependencies": {
        "@mui/x-date-pickers": "^6.18.1",
        "@react-buddy/ide-toolbox": "^2.3.1",
        "@react-buddy/palette-mui": "^5.0.2",
        "@reduxjs/toolkit": "^1.9.7",
        "axios": "^1.4.0",
        "dayjs": "^1.11.10",
        "js-cookie": "^3.0.5",
        "prettier-linter-helpers": "^1.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.18.0",
        "vite-plugin-checker": "^0.6.2"
    },
    "devDependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.16",
        "@mui/material": "^5.14.17",
        "@mui/x-data-grid": "^6.18.0",
        "@redux-devtools/extension": "^3.2.5",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/crypto-js": "^4.1.1",
        "@types/node": "^20.2.5",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/redux-logger": "^3.0.11",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.0",
        "@vitest/coverage-c8": "^0.33.0",
        "dotenv": "^16.1.4",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "jsdom": "^22.1.0",
        "json-server": "^0.17.4",
        "prettier": "3.0.0",
        "redux-logger": "^3.0.6",
        "tss-react": "^4.9.3",
        "typescript": "^5.0.2",
        "vite": "^4.3.0",
        "vitest": "^0.30.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
